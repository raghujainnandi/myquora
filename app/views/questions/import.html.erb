<div class="container">
  <div class="row">
    <div class="col-md-8 offset-md-2">

      <h4> Import Questions !! </h4>
      <br/>
      <%= form_tag import_questions_path, multipart: true do %>
        <%= file_field_tag :file, class: "form-control" %>
        <br />
        <%= submit_tag "Import from file", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>